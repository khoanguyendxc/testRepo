{
  "name": "latitude-service-centre",
  "version": "1.21.1",
  "description": "Latitude Service Centre",
  "license": "MIT",
  "author": "Latitude Financial Services",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production react-scripts build",
    "postbuild": "node ./scripts/post-build.js",
    "build:dev": "react-scripts build",
    "build:local": "yarn copy:build:local && react-scripts build && concurrently \"lcp --proxyUrl https://api.test.latitudefinancial.com\" \"serve -s build -p 8000\"",
    "commit": "cz",
    "copy:build:local": "cp .env.test .env.production.local",
    "copy:local": "cp .env.localmachine .env.development",
    "copy:nonprod": "cp .env.nonprod .env.production",
    "copy:nonprodmocks": "cp .env.nonprodmocks .env.production",
    "copy:test": "cp .env.test .env.development",
    "cypress:dev": "TZ=UTC cypress open --config baseUrl=\"http://localhost:8000\"",
    "cypress:run": "TZ=UTC cypress run --config baseUrl=\"http://localhost:8000\" --browser chrome",
    "cypress:run:ci": "TZ=UTC cross-env cypress run --config screenshotOnRunFailure=false,watchForFileChanges=false,baseUrl=\"http://localhost:8000\" --browser chrome --headless --spec $npm_config_group ",
    "datadog:ci": "datadog-ci sourcemaps upload ./build --service=$REACT_APP_DATADOG_SERVICE --release-version=$APP_VERSION --minified-path-prefix=/static/js",
    "e2e": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn cypress:run\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "e2e:ci": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn e2e:ci:grouped\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "e2e:ci-master-only": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn cypress:run:ci --spec='cypress/integration/group-master-only/*.spec.js'\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "e2e:ci1": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn cypress:run:ci --spec='cypress/integration/group1/*.spec.js'\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "e2e:ci2": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn cypress:run:ci --spec='cypress/integration/group2/*.spec.js'\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "e2e:ci3": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn cypress:run:ci --spec='cypress/integration/group3/*.spec.js'\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "e2e:ci4": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn cypress:run:ci --spec='cypress/integration/group4/*.spec.js'\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "e2e:ci5": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn cypress:run:ci --spec='cypress/integration/group5/*.spec.js'\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "e2e:ci:grouped": "concurrently \"yarn cypress:run:ci --group='cypress/integration/group1/*.spec.js'\" \"yarn cypress:run:ci --group='cypress/integration/group2/*.spec.js'\" \"yarn cypress:run:ci --group='cypress/integration/group3/*.spec.js'\"",
    "e2e:head": "concurrently --kill-others --success first \"wait-on http://localhost:8000/ --timeout 300000 && yarn cypress:dev\" \"BROWSER=none HOST=localhost yarn start:local:stub\"",
    "eject": "react-scripts eject",
    "postinstall": "cd test-server/mock-api && yarn",
    "prettier": "prettier --write \"**/*.{js,json,html}\"",
    "release": "standard-version",
    "release-rc": "standard-version --prerelease rc",
    "serve": "yarn build && serve -s build -l 8000",
    "server": "cross-env NODE_ENV=development DEBUG=api node test-server/mock-api/index.js",
    "spec": "yarn cypress open-ct",
    "spec:ci": "npx cypress run-ct --browser chrome --headless",
    "spellcheck": "cspell \"src/**/*.{js,jsx,ts,tsx}\" --no-summary --no-progress",
    "start": "PORT=8000 react-scripts start",
    "start:local": "yarn copy:local && yarn start",
    "start:local:stub": "yarn copy:local && concurrently \"yarn server\" \"yarn start\"",
    "start:test": "yarn copy:test && concurrently \"lcp --proxyUrl https://api.test.latitudefinancial.com\" \"yarn start\"",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu DEBUG_PRINT_LIMIT=100000 TZ=UTC react-scripts test --testPathIgnorePatterns=__tests__ --transformIgnorePatterns \"node_modules/(?!@codemirror)/\"",
    "typecheck": "tsc --noEmit",
    "verify": "eslint --max-warnings=0 . -c .eslintrc-ci && yarn typecheck && yarn spellcheck",
    "verify:fix": "eslint . -c .eslintrc-ci --max-warnings=0 --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-scripts/eslint-webpack-plugin": "2.4.1"
  },
  "dependencies": {
    "@datadog/browser-rum": "3.6.12",
    "@emotion/react": "11.5.0",
