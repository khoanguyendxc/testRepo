    "react": "17.0.2",
    "react-currency-masked-input": "2.4.0",
    "react-day-picker": "8.0.0-beta.29",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.3",
    "react-event-tracker": "5.2.0",
    "react-inlinesvg": "2.0.0",
    "react-input-mask": "2.0.4",
    "react-modal": "3.8.1",
    "react-number-format": "3.3.4",
